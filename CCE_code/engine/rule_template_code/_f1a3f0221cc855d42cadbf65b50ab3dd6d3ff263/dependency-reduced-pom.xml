<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>cce</groupId>
  <artifactId>_f1a3f0221cc855d42cadbf65b50ab3dd6d3ff263</artifactId>
  <version>1.0.0</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.6.0</version>
        <configuration>
          <source>11</source>
          <target>11</target>
          <encoding>UTF-8</encoding>
          <forceJavacCompilerUse>true</forceJavacCompilerUse>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <id>shade</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>invoke</id>
      <properties>
        <skipLocalInvoke>false</skipLocalInvoke>
      </properties>
    </profile>
  </profiles>
  <dependencies>
    <dependency>
      <groupId>com.amazonaws</groupId>
      <artifactId>aws-java-sdk-bom</artifactId>
      <version>1.11.579</version>
      <type>pom</type>
      <scope>import</scope>
    </dependency>
  </dependencies>
  <properties>
    <awsRegion>sa-east-1</awsRegion>
    <stackName>${project.groupId}</stackName>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <s3Prefix>${project.artifactId}</s3Prefix>
    <skipLocalInvoke>true</skipLocalInvoke>
    <s3Bucket>cce-bucket</s3Bucket>
  </properties>
</project>

